{
  "name": "@brandlen/prettier-config",
  "type": "module",
  "version": "0.1.2",
  "author": "Brandon Leon <https://github.com/Brandon-Ln>",
  "description": "Brandon Leon's personal prettier config",
  "main": "src/index.cjs",
  "module": "src/index.js",
  "packageManager": "pnpm@8.0.0",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.cjs"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "lint": "prettier src/ -w --cache",
    "release": "release-it --dry-run --no-github.release && release-it"
  },
  "peerDependencies": {
    "prettier": "^2.0.0 || ^3.0.0"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "release-it": "^14.14.0"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "JavaScript Standard Style",
    "style",
    "prettier",
    "prettierconfig",
    "prettier-config"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brandon-Ln/prettier-config.git"
  },
  "homepage": "https://github.com/Brandon-Ln/prettier-config#readme"
}
